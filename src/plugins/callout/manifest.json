{
    "name": "Callout",
    "version": "1.0.0",
    "author": "Alexandre Tavares",
    "description": "Adds callout blocks.",
    "entries": [
        {
            "path": "./extensions/slash-menu-callout-extension.tsx",
            "class": "SlashMenuCalloutExtension",
            "description": "Adds callout block to the slash menu.",
            "active": true
        },
        {
            "path": "./extensions/block-options-callout-extension.tsx",
            "class": "BlockOptionsCalloutExtension",
            "description": "Adds callout background color item to the block options menu.",
            "active": true
        },
        {
            "path": "./extensions/mobile-toolbar-callout-extension.tsx",
            "class": "CalloutMobileToolbarExtension",
            "description": "Adds a mobile toolbar button for selecting callout colors.",
            "active": true
        },
        {
            "path": "./extensions/callout-command-extension.ts",
            "class": "CalloutCommandExtension",
            "description": "Adds callout commands. E.g., openCalloutColorOptions.",
            "active": true
        },
        {
            "path": "./extensions/callout-enter-plugin.tsx",
            "class": "CalloutEnterPlugin",
            "description": "Handles Enter key behavior inside callout blocks.",
            "active": true
        }
    ]
}